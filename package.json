	"description": "Observational-memory compaction strategy for pi with observer/reflector token thresholds",
	"type": "module",
	"license": "MIT",
	"keywords": [
		"pi-package",
		"pi",
		"pi-coding-agent",
		"extension",
		"memory",
		"compaction",
		"observational-memory"
	],
	"files": [
		"index.ts",
		"overlay.ts",
		"README.md",
		"DESIGN.md",
		"package.json"
	],
	"scripts": {
